{
  "name": "my-project",
  "jest": {
    "verbose": true
  },
  "transform": {
    "^.+\\.jsx?$": "babel-jest"
  },
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "statements": 100
    }
  },
  "collectCoverage": true,
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/services/fixtures/*.{js,jsx}"
  ]
}
